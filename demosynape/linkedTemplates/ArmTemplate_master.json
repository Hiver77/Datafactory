{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demosynape"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Local_PC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Local_PC'"
		},
		"Local_PC_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\DESKTOP-5F6DJ1M\\Users\\user\\Documents\\FGA"
		},
		"Local_PC_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "user"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demosynape_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Local_PC_password": {
						"value": "[parameters('Local_PC_password')]"
					},
					"Local_PC_properties_typeProperties_host": {
						"value": "[parameters('Local_PC_properties_typeProperties_host')]"
					},
					"Local_PC_properties_typeProperties_userId": {
						"value": "[parameters('Local_PC_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}